      "source": [
        "import random\n",
        "\n",
        "var = random.randint(0,100) #正の整数は0～100とする\n",
        "print(var)\n",
        "\n",
        "if var % 3 == 0 and var % 5 == 0:\n",
        "  print(\"FizzBuzz\") #3の倍数かつ5の倍数であるとき\n",
        "elif var % 3 == 0:\n",
        "  print(\"Fizz\") #3の倍数であるとき\n",
        "elif var % 5 == 0:\n",
        "  print(\"Buzz\") #5の倍数であるとき\n",
        "else:\n",
        "  print(var) #それ以外"
      ]
    }
